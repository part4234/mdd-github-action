Repository {
	workflows {
		
		Workflow {
			name "Feature Branch"
			on {
				PushEvent {
					branches { master, "release*" }
				}
			}
			jobs {
				Job test {
					name "macos-latest"
					runsOn "ubuntu-18.04"
					steps {
						Step {
							uses "actions/checkout@v1"
						},
						Step {
							name "Set up JDK 11"
							uses "actions/setup-java@v1"
							with { "java-version": "1.11" }
						},
						Step {
							name "Maven Package"
							run { "mvn -B clean package -DskipTests" }
						}
					}
				}
			}
		}
	}
}